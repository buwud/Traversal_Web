@model List<GetAllGuideQueryResult>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>CQRS MediatR Guide List</h1>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Name</th>
        <th>Description</th>
        <th>Delete</th>
        <th>Edit</th>
    </tr>
    @foreach(var item in Model)
    {
    <tr>
        <td>@item.GuideID</td>
        <td>@item.Name</td>
        <td>@item.Description</td>
        <td><a href="#" class="btn btn-outline-danger">Delete</a></td>
        <td><a href="#" class="btn btn-outline-success">Edit</a></td>
    </tr>
    }
</table>
<a href="#" class="btn btn-outline-primary">Add More</a>